{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://bridge-sec-project-frontend.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Timezone"
        },
        "dest": "/api/$1"
      }
    ]
  }
  