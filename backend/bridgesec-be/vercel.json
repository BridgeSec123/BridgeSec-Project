{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "https://bridge-sec-project-backend.vercel.app/$1",
        "headers": {
          "Access-Control-Allow-Origin": "https://bridge-sec-project-frontend.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Timezone, application/json"
        }
      }
    ]
  }
  